{
  "env": {
    "nrf5_sdk.path": "/home/nick/projects/iot/nRF5_SDK_17_0_2/"
  },
  "configurations": [
    {
      "name": "nRF52840 DK",
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/lib/arm-none-eabi/include",
        "${env:nrf5_sdk.path}/modules/**",
        "${env:nrf5_sdk.path}/components/**"
      ],
      "forcedInclude": ["${workspaceFolder}/config/sdk_config.h"],
      "defines": [
        "BOARD_PCA10056",
        "CONFIG_GPIO_AS_PINRESET",
        "INITIALIZE_USER_SECTIONS",
        "FLOAT_ABI_HARD",
        "NRF52",
        "NRF52840_XXAA",
        "NRF_SD_BLE_API_VERSION=6",
        "S140",
        "SOFTDEVICE_PRESENT",
        "SWI_DISABLE0"
      ],
      "compilerPath": "/usr/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "clang-x64",
      "browse": {
        "path": [
          "/usr/lib/arm-none-eabi/include",
          "${env:nrf5_sdk.path}/**",
          "${workspaceRoot}"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": ""
      }
    }
  ],
  "version": 4
}
